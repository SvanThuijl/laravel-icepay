{
    "name": "svanthuijl/laravel-icepay",
    "description": "A Laravel wrapper for the ICEPAY REST API",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "icepay/api": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Svanthuijl\\Icepay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Svanthuijl\\Icepay\\IcepayServiceProvider"
            ],
            "aliases": {
                "Icepay": "Svanthuijl\\Icepay\\Facades\\Icepay"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
